{
  "name": "simple-backend-nest-service",
  "version": "0.2.1",
  "description": "An exemplar simple backend service written in TypeScript and https://github.com/nestjs/nest",
  "author": "Application Infrastructure Team (#team-application-infrastructure)",
  "private": true,
  "license": "Private",
  "engines": {
    "node": "~16",
    "npm": "~7"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "depcheck": "depcheck",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,test}/**/*.ts\" --fix",
    "lint:check": "eslint \"{src,apps,test}/**/*.ts\"",
    "lint:staged": "lint-staged -c lint-staged.config.js",
    "report-sourcemaps": "bugsnag-source-maps upload-node --api-key $BUGSNAG_API_KEY --app-version $APP_VERSION --directory .",
    "report-build": "bugsnag-build-reporter -k $BUGSNAG_API_KEY -v $APP_VERSION",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/e2e/jest.e2e.config.js",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "doc": "npx @compodoc/compodoc -p tsconfig.json",
    "doc:server": "npx @compodoc/compodoc -p tsconfig.json -s",
    "husky:install": "husky install",
    "prepare": "./prepare.sh"
  },
  "dependencies": {
    "@nestjs/common": "^8.0.6",
    "@nestjs/config": "^1.0.1",
    "@nestjs/terminus": "^8.0.1",
    "@procore/foundations-nest": "^2.1.0"
  },
  "devDependencies": {
    "@bugsnag/source-maps": "^2.3.0",
    "@compodoc/compodoc": "^1.1.16",
    "@nestjs/cli": "^8.1.4",
    "@nestjs/schematics": "^8.0.5",
    "@nestjs/testing": "^8.1.2",
    "@procore/core-prettier": "^10.0.0",
    "@types/jest": "^27.0.3",
    "@types/node": "^16.11.11",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.5.0",
    "@typescript-eslint/parser": "^5.4.0",
    "bugsnag-build-reporter": "^2.0.0",
    "depcheck": "^1.4.2",
    "eslint": "^8.3.0",
    "eslint-config-prettier": "^8.3.0",
    "husky": "^7.0.4",
    "jest": "27.3.1",
    "jest-chain": "^1.1.5",
    "jest-extended": "^1.1.0",
    "jest-junit": "^13.0.0",
    "lint-staged": "^12.1.2",
    "prettier": "^2.4.1",
    "rimraf": "^3.0.2",
    "supertest": "6.1.6",
    "ts-jest": "^27.0.7",
    "ts-loader": "^9.2.6",
    "ts-node": "10.4.0",
    "tsconfig-paths": "^3.11.0",
    "typescript": "^4.4.4"
  }
}
